{
  "name": "heyday/silverstripe-cacheinclude",
  "type": "silverstripe-vendormodule",
  "description": "Fast Caching for SilverStripe",
  "require": {
    "silverstripe/framework": "^5.0.0",
    "symfony/expression-language": "^6.3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "Heyday\\CacheInclude\\": "src/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "5.x-dev"
    }
  },
  "prefer-stable": true,
  "minimum-stability": "dev",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
