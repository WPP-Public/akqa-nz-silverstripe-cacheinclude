{
    "name": "heyday/silverstripe-cacheinclude",
    "type": "silverstripe-vendormodule",
    "description": "Fast Caching for SilverStripe",
    "authors": [
        {
            "name": "Cam Spiers",
            "email": "cameron@heyday.co.nz",
            "homepage": "https://github.com/camspiers/"
        },
        {
            "name": "Stephen Holdaway",
            "email": "stephen.holdaway@heyday.co.nz",
            "homepage": "https://github.com/stecman/"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.2",
        "symfony/yaml": "^3.2 || ^4",
        "symfony/expression-language": "^3.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7"
    },
    "autoload": {
        "psr-4": {
            "Heyday\\CacheInclude\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
